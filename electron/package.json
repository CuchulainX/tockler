{
    "name": "tockler",
    "version": "3.3.17",
    "description": "Automatically track applications usage and working time",
    "author": "Maigo Erit <maigo.erit@gmail.com>",
    "license": "GPL-2.0",
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "git@github.com:MayGo/tockler.git"
    },
    "scripts": {
        "tslint": "tslint --project .",
        "start": "nps",
        "test": "nps test",
        "____pre____": "",
        "postinstall": "electron-builder install-app-deps",
        "build_mac": "build -c electron-builder.yml --mac",
        "build_linux": "build -c electron-builder.yml --linux",
        "build_windows": "build -c electron-builder.yml --win",
        "build": "webpack --progress --env.production",
        "release": "cpx \"../client/build/**/*\" dist && build -c electron-builder.yml"
    },
    "dependencies": {
        "active-win": "^3.0.0",
        "compare-versions": "^3.0.0",
        "cpx": "^1.5.0",
        "delay": "^2.0.0",
        "electron-config": "^1.0.0",
        "electron-context-menu": " 0.9.1",
        "electron-devtools-installer": "^2.2.4",
        "electron-is-dev": "^0.3.0",
        "electron-log": "^2.2.15",
        "electron-squirrel-startup": "^1.0.0",
        "electron-updater": "2.21.10",
        "hazardous": "^0.3.0",
        "menubar": "^5.2.3",
        "moment": "2.18.1",
        "multiline": "^1.0.2",
        "node-notifier": "^5.0.2",
        "os-locale": "^2.1.0",
        "randomcolor": "0.5.3",
        "requirejs": "^2.3.3",
        "sequelize": "4.4.2",
        "sequelize-mock": "^0.9.0",
        "sqlite3": "^3.1.9"
    },
    "peerDependencies": {},
    "devDependencies": {
        "@types/jest": "^20.0.6",
        "@types/lodash": "^4.14.72",
        "@types/node": "^8.0.20",
        "@types/sequelize": "^4.0.61",
        "@types/webdriverio": "^4.7.1",
        "@types/webpack": "^3.0.7",
        "awesome-typescript-loader": "^3.2.2",
        "cross-env": "^5.0.0",
        "electron": "2.0.2",
        "electron-builder": "20.15.1",
        "electron-connect": "0.6.3",
        "electron-reload": "1.2.2",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "jest": "20.0.4",
        "jest-cli": "20.0.4",
        "lodash": "^4.17.4",
        "nps": "^5.7.0",
        "nps-utils": "^1.3.0",
        "ts-jest": "^20.0.10",
        "ts-node": "^3.3.0",
        "typescript": "^2.4.2",
        "wait-on": "2.0.2",
        "webpack": "^3.5.2",
        "webpack-node-externals": "^1.6.0",
        "tslint": "^5.9.1"
    },
    "jest": {
        "globals": {
            "__TEST__": true
        },
        "unmockedModulePathPatterns": [
            "babel",
            "<rootDir>/node_modules/source-map-support"
        ],
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "setupTestFrameworkScriptFile": "<rootDir>/util/jest-setup.js",
        "transform": {
            "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "/__tests__/.*\\.test\\.ts$",
        "testEnvironment": "node"
    }
}
